    "import torch\n",
    "x = torch.tensor(2.0, requires_grad=True)\n",
    "z = torch.tensor(4.0, requires_grad=True)\n",
    "\n",
    "# define function\n",
    "y = x**2+z**3\n",
    "\n",
    "y.backward()    # get Derivatives\n",
    "print(x.grad)   # tensor(4.)\n",
    "print(z.grad)   # tensor(48.)"
   ]
  }
